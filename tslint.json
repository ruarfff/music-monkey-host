{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "**/*.js"]
  },
  "rules": {
    "no-console": [false],
    "object-literal-sort-keys": false,
    "no-var-requires": false
  },
  "defaultSeverity": "warning"
}
