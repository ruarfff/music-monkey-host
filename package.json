    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/history": "^4.6.2",
    "@types/jest": "22.2.3",
    "@types/jquery": "^3.3.5",
    "@types/lodash": "^4.14.108",
    "@types/node": "10.0.3",
    "@types/prop-types": "^15.5.2",
    "@types/pusher-js": "^4.2.0",
