    "react-beautiful-dnd": "^9.0.1",
    "react-cookie": "^2.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-dropzone": "^4.2.9",
    "react-dropzone-component": "^3.2.0",
    "react-jss": "^8.6.1",
    "react-loading": "^2.0.2",
    "react-places-autocomplete": "^7.0.1",
    "react-pure-lifecycle": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-swipeable-views": "^0.12.17",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "spotify-uri": "^1.0.0",
    "spotify-web-api-js": "^1.1.0",
