{
  "name": "music-monkey-host",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV='development' REACT_APP_AUTH_SUFFIX='-local' npm-run-all -p watch-css start-js",
    "start:local": "NODE_ENV='development' REACT_APP_AUTH_SUFFIX='-local-dev' REACT_APP_MM_API_URL='http://localhost:8080/api/v1' react-app-rewired start",
    "start-js": "react-app-rewired start",
    "start-local": "NODE_ENV='development' REACT_APP_AUTH_SUFFIX='-local'",
    "build": "NODE_ENV='production' npm-run-all build-css build-js",
    "build-js": "react-app-rewired build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-app-rewired eject",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "lint": "tslint --fix src/**/*.ts{,x}"
  },
  "dependencies": {
